class Solution {
    public int search(int[] nums, int target) {
        if(nums == null)
            return -1;
        int start = 0;
        int end = nums.length - 1;
        while(start <= end){
            int mid = start + (end - start) / 2;
            if(nums[mid] == target){
                return mid;
            }
            else if(nums[mid] < target){
                if(nums[mid] >= nums[start]){
                    start = mid + 1;
                }
                else{
                    if(target <= nums[end]){
                        start = mid + 1;
                    }
                    else{
                        end = mid - 1;
                    }
                }
            }
            else{
                if(target >= nums[start]){
                    end = mid - 1;
                }
                else{
                    if(nums[mid] >= nums[start]){
                        start = mid + 1;
                    }
                    else{
                        end = mid - 1;
                    }
                }
            }
        }
        return -1;
    }
}
